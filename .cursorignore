# Add directories or file patterns to ignore during indexing (e.g. foo/ or *.csv)
{
  "version": "1.0",
  "rules": {
    "priority": [
      "environment.mdc",
      "globalIgnore.mdc"
    ],
    "ignoreGitIgnore": true
  },
  "editor": {
    "allowedPatterns": [
      ".env*"
    ]
  },
  "security": {
    "allowEnvEditing": true
  }
}
